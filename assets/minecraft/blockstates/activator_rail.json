{
	"multipart": [
		{
			"when": { "shape": "north_south", "powered": false },
			"apply": { "model": "block/activator_rail" }
		},
		{
			"when": { "shape": "east_west", "powered": false },
			"apply": { "model": "block/activator_rail", "y": 90 }
		},
		{
			"when": { "shape": "ascending_north", "powered": false },
			"apply": { "model": "block/activator_rail_ascending" }
		},
		{
			"when": { "shape": "ascending_east", "powered": false },
			"apply": { "model": "block/activator_rail_ascending", "y": 90 }
		},
		{
			"when": { "shape": "ascending_south", "powered": false },
			"apply": { "model": "block/activator_rail_ascending", "y": 180 }
		},
		{
			"when": { "shape": "ascending_west", "powered": false },
			"apply": { "model": "block/activator_rail_ascending", "y": 270 }
		},
		{
			"when": { "shape": "north_south", "powered": true },
			"apply": { "model": "block/activator_rail_on" }
		},
		{
			"when": { "shape": "east_west", "powered": true },
			"apply": { "model": "block/activator_rail_on", "y": 90 }
		},
		{
			"when": { "shape": "ascending_north", "powered": true },
			"apply": { "model": "block/activator_rail_on_ascending" }
		},
		{
			"when": { "shape": "ascending_east", "powered": true },
			"apply": { "model": "block/activator_rail_on_ascending", "y": 90 }
		},
		{
			"when": { "shape": "ascending_south", "powered": true },
			"apply": { "model": "block/activator_rail_on_ascending", "y": 180 }
		},
		{
			"when": { "shape": "ascending_west", "powered": true },
			"apply": { "model": "block/activator_rail_on_ascending", "y": 270 }
		},
		{
			"when": { "shape": "north_south" },
			"apply": { "model": "block/rail" }
		},
		{
			"when": { "shape": "east_west" },
			"apply": { "model": "block/rail", "y": 90 }
		},
		{
			"when": { "shape": "ascending_north" },
			"apply": { "model": "block/rail_ascending" }
		},
		{
			"when": { "shape": "ascending_east" },
			"apply": { "model": "block/rail_ascending", "y": 90 }
		},
		{
			"when": { "shape": "ascending_south" },
			"apply": { "model": "block/rail_ascending", "y": 180 }
		},
		{
			"when": { "shape": "ascending_west" },
			"apply": { "model": "block/rail_ascending", "y": 270 }
		}
	]
}